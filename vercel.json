{
    "version": 2,
    "builds": [
      {
        "src": "index.js", 
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/server.js" 
      }
    ],
    "env": {
      "DATABASE_URL": "@your-database-url",
      "OTHER_ENV_VAR": "@your-other-env-var"
    }
  }
  